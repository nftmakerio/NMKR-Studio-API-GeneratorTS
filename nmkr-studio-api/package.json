{
  "name": "nmkr-studio-api",
  "version": "1.0.0",
  "description": "Typesafe API Client to work with the NMKR Studio V2 API",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json --setupFiles dotenv/config",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A ./..",
    "postversion" : "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
],
  "keywords": ["NMKR", "API","STUDIO"],
  "author": "schaier-io",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nftmakerio/NMKRApi/tree/main/nmkr-studio-api"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "dotenv": "^16.0.3",
    "prettier": "^2.8.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
